<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.angzhao.dao.addressInfoDao">
    <select id="queryByUserId" resultType="com.angzhao.entity.addressEntity" parameterType="string">
        SELECT *
        FROM address_info
        WHERE user_id = #{userId}
    </select>
</mapper>